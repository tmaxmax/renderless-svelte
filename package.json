{
  "name": "renderless-svelte",
  "version": "1.5.0",
  "description": "A collection of renderless Svelte components",
  "homepage": "https://www.renderless-svelte.dev",
  "repository": {
    "url": "https://github.com/stephane-vanraes/renderless-svelte"
  },
  "svelte": "src/index.ts",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "format": "prettier -w --plugin-search-dir=. src",
    "validate": "svelte-check src"
  },
  "keywords": [
    "svelte",
    "components",
    "renderless"
  ],
  "author": "Stephane Vanraes",
  "license": "ISC",
  "files": [
    "src",
    "dist"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@tsconfig/svelte": "^1.0.10",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.41.5",
    "rollup-plugin-svelte": "^7.1.0",
    "sveld": "^0.7.1",
    "svelte": "^3.35.0",
    "svelte-check": "^1.2.5",
    "svelte-preprocess": "^4.6.9",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  }
}
